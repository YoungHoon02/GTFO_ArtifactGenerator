<Project>
  <PropertyGroup>
    <!-- GTFO Game Installation Path -->
    <GTFOPath>D:\SteamLibrary\steamapps\common\GTFO</GTFOPath>

    <!-- r2modman Profile Path (using r2modman for mod management) -->
    <R2ModManProfile>C:\Users\User\AppData\Roaming\r2modmanPlus-local\GTFO\profiles\Default</R2ModManProfile>

    <!-- BepInEx Installation Path (r2modman manages BepInEx) -->
    <BepInExPath>$(R2ModManProfile)\BepInEx</BepInExPath>

    <!-- IL2CPP DummyDlls Path (generated by BepInEx IL2CPP) -->
    <DummyDllsPath>$(BepInExPath)\interop</DummyDllsPath>
  </PropertyGroup>
</Project>
